		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"@vscode/test-electron": "^1.6.1",
		"eslint": "^8.26.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"source-map-support": "^0.5.12",
		"ts-loader": "^9.4.2",
		"typescript": "^4.9.4",
		"webpack": "^5.75.0",
